on chat:
	player has permission "staff.member" or "normal.friend"
	wait 5 ticks
	if message contains "!discord":
		broadcast "{@P} &ddiscord.ponderuhc.cf"
	else if message contains "!ip":
		broadcast "{@P} &dplay.ponderuhc.cf"
	else if message contains "!store":
